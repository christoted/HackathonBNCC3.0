<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_fragment_shape"
    android:paddingTop="10dp"
    android:paddingLeft="9dp"
    android:paddingRight="9dp">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/id_rv_fragment_my_ticket_yourConcert"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:background="@color/white"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_tv_fragment_home_yourConcert"
        app:layout_constraintVertical_bias="0.0"
        tools:listitem="@layout/item_home_concert"/>
</LinearLayout>