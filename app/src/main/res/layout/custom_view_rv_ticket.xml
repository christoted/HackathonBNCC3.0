<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id_constraintLayout_myticket"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="68dp" >


    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/id_img_customview_content"
        android:layout_width="118dp"
        android:layout_height="219dp"
        app:srcCompat="@drawable/ic_launcher_background"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/id_tv_customview_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Title"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="33dp" />

    <TextView
        android:id="@+id/id_tv_customview_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="This is content"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="59dp" />

    <TextView
        android:id="@+id/id_tv_customview_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="18 oktober 2020"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="91dp" />

    <TextView
        android:id="@+id/id_tv_customview_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="13.00 - 15.00"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="131dp" />

    <TextView
        android:id="@+id/id_tv_customview_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Online"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="172dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="68dp">


    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/id_img_customview_date"
        android:layout_width="33dp"
        android:layout_height="26dp"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="84dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/id_img_customview_time"
        android:layout_width="30dp"
        android:layout_height="30dp"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="120dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/id_img_customview_type"
        android:layout_width="30dp"
        android:layout_height="30dp"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="161dp"
        tools:srcCompat="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>